{
  "name": "benin-dev-club-api",
  "version": "0.0.1",
  "description": "benin dev club",
  "author": "python-benin",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run typeonly && nest build",
    "dev": "npm run typeonly && NODE_ENV=development nest start --watch --preserveWatchOutput | pino-pretty -t 'yyyy-mm-dd HH:MM:ss.l' -i 'hostname,pid'",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "typeonly": "typeonly --bundle dist/config.to.json src/configuration-types.d.ts",
    "lint": "rome check ./",
    "lint:apply": "rome check ./ --apply",
    "lint:apply-suggested": "rome check ./ --apply-suggested",
    "format:check": "rome format ./",
    "format": "rome format ./ --write"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@typeonly/validator": "^0.5.2",
    "apollo-server-express": "^3.11.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "graphql": "^16.6.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sqlite3": "^5.1.4",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/node": "18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "concurrently": "^7.6.0",
    "pino-pretty": "^9.1.1",
    "rome": "^11.0.0",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typeonly": "^0.4.6",
    "typescript": "^4.7.4"
  }
}
